msgid "Excellent"
msgstr "Exzellent"

#: ../src/runType.cs:238
msgid "Good"
msgstr "Gut"

#: ../src/runType.cs:239
msgid "Average"
msgstr "Durchschnittlich"

#: ../src/runType.cs:240
msgid "Fair"
msgstr "Ausreichend"

